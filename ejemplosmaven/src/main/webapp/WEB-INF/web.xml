<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<filter>
		<filter-name>primerFiltro</filter-name>
		<display-name>primerFiltro</display-name>
		<description></description>
		<filter-class>com.filters.primerFiltro</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>primerFiltro</filter-name>
		<url-pattern>/primerFiltro</url-pattern>
	</filter-mapping>
	
	<servlet>
		<servlet-name>PrimerController</servlet-name>
		<display-name>PrimerController</display-name>
		<description></description>
		<servlet-class>controllers.PrimerController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>PrimerController</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>/</welcome-file>
		<welcome-file>home.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	
	</welcome-file-list>
</web-app>
